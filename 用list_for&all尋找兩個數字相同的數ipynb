        "# find n1 and n2 數字相同(最小)\n",
        "# ex n1 = 10255 n2 = 20510\n",
        "n = 1\n",
        "\n",
        "while n  :\n",
        "  n1 = str(n)\n",
        "  n2 = str(n*2)\n",
        "  if all([i in n2 for i in n1]+\n",
        "      [i in n1 for i in n2]):\n",
        "    break\n",
        "  n+=1\n",
        "print(n)\n",
        "\n",
        "# Result 10255"
      ]
    }
